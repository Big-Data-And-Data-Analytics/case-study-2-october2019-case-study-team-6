{
  "manifest_version": 2,

  "name": "Predictor",
  "description": "This plugin will predict either the national identity or idenity motive of a selected comment.",
  "version": "1.0",

  "browser_action": {
   "default_icon": {
     "16": "icon.png",
     "32": "icon.png",
     "48": "icon.png",
     "128": "icon.png"
   },
   "default_popup": "buttons_ui.html"
  },
  "permissions": [
   "activeTab",
   "*://*.twitter.com/*",
   "<all_urls>",
   "tabs",
   "storage"
   ]
}